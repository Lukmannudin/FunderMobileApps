<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.team.oleg.funder">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
            android:name="com.team.oleg.funder.MyCustomApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.team.oleg.funder.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.team.oleg.funder.Login.LoginActivity">
        </activity>
        <activity android:name="com.team.oleg.funder.Home.HomeActivity">
        </activity>
        <activity android:name="com.team.oleg.funder.Chat.ChatActivity">
        </activity>
        <activity android:name="com.team.oleg.funder.DealHistory.DealHistoryActivity">
        </activity>

    </application>


</manifest>