<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.team.oleg.funder">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:name=".MyCustomApplication"
            android:allowBackup="true"
            android:icon="@drawable/appicon"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Company.EOProfile.EoProfileActivity"
                  android:theme="@style/AppTheme.NoActionBar"
        >
        </activity>
        <activity android:name=".Company.DealForm.DealFormActivity"
                  android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        >
        </activity>
        <activity android:name=".Company.RequestDetail.RequestDetailActivity">
        </activity>
        <activity
                android:name=".Login.LoginPresenter"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Login.LoginEO.LoginEOActivity"/>
        <activity
                android:name=".Main.MainActivity"
                android:theme="@style/AppTheme.NoActionBar"/>
        <activity
                android:name=".EventOrganizer.SearchHome.SearchHomeActivity"
                android:theme="@style/AppTheme.NoActionBar"/>
        <activity
                android:name=".Auction.AuctionActivity"
                android:parentActivityName=".Main.MainActivity"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".EventOrganizer.FillForm.FillFormActivity"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".EventOrganizer.EventOrganizerProfile"
                android:theme="@style/Theme.MyEOProfile">
        </activity>
        <activity
                android:name=".Login.LoginCompany.LoginCompanyActivity"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".Login.SignUp.SignUpActivity"
                android:parentActivityName=".Login.LoginEO.LoginEOActivity">
        </activity>
        <activity android:name=".Company.CompanyActivity">
        </activity>
        <activity
                android:name=".EventOrganizer.ChatMessageEO.ChatMessageEOActivity"
                android:parentActivityName=".Main.MainActivity">
        </activity>
        <activity
                android:name=".Company.ChatMessageCompany.ChatMessageCompanyActivity"
                android:parentActivityName=".Company.CompanyActivity"
                android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
    </application>

</manifest>